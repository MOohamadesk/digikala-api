{
  "name": "digi-mock-api",
  "version": "1.0.0",
  "description": "Digikala Mock API",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:css": "npx tailwindcss -i ./src/style/input.css -o ./src/style/output.css --watch",
    "build:css": "npx tailwindcss -i ./src/style/input.css -o ./src/style/output.css --minify",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "daisyui": "^5.5.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.0",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "swiper": "^12.0.3"
  },
  "homepage": "https://moohamadesk.github.io/digi-mock-api"
}