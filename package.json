{
  "name": "digi-json",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:css": "npx tailwindcss -i ./src/style/input.css -o ./src/style/output.css --watch",
    "build:css": "npx tailwindcss -i ./src/style/input.css -o ./src/style/output.css --minify",
    "server": "json-server db.json --port 3000",
    "start": "npm-run-all --parallel dev dev:css server"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "daisyui": "^5.5.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.0",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "json-server": "^0.17.4",
    "swiper": "^12.0.3",
    "vercel": "^49.1.2"
  }
}
